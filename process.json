{
    "apps": [
        {
            "name": "yolo",
            "script": "index.js",
            "merge_logs": true,
            "max_restarts": 20,
            "instances": 1,
            "out_file" : "/tmp/yolo.log",
            "log_date_format" : "DD/MM/YYYY HH:mm:ss",
            "max_memory_restart": "1G",
            "env": {
                "NODE_ENV": "prod"
            }
        }
    ]
}